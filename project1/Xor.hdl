/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Or(a=a,b=b,out=outOr);
	Not(in=a,out=notA);
	Not(in=b,out=notB);
	Or(a=notA,b=notB,out=negOutOr);
	And(a=outOr,b=negOutOr,out=out);
}